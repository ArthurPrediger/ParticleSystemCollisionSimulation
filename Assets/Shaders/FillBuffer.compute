// Each #kernel tells which function to compile; you can have many kernels
#pragma kernel FillBuffer

AppendStructuredBuffer<int> buffer;
int numElements;

[numthreads(1,1,1)]
void FillBuffer (uint3 id : SV_DispatchThreadID)
{
    for (int i = 0; i < numElements; i++)
    {
        buffer.Append(-1);
    }
}
